#include "pch.h"
#include "iostream"
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int q, w;
	double e, r;
	cout << "Введите 3начения переменной int: "<< endl;
	cin >> q;
	cin >> w;
	cout << "Введите 3начения переменной double: " << endl;
	cin >> e; 
	cin >> r;
	cout << "Вывод: " << endl << q << endl << w << endl << e << endl << r << endl ;
	system("pause");
	return 0;
}
